{%
	set classes = [
		dom_id ? 'js-view-dom-id-' ~ dom_id,
	]
%}

<div{{ attributes.addClass(classes) }}>

	<div class="card peoples">

		<div class="card__item card__header spaced--less">

			<div class="ribbon med">
				<div class="ribbon__segments">
					<div class="ribbon__segment left">

						<h4 class="title title--secondary tt--u inline-block">People</h4>

					</div>
				</div>
			</div>

		</div>

		<div class="card__item card__body">

			{{ rows }}

		</div>

	{{ feed_icons }}

</div>